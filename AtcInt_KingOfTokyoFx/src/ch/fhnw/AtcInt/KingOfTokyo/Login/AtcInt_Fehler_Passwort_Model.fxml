<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="270.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.fhnw.AtcInt.KingOfTokyo.Login.AtcInt_Fehler_Passwort_Controller">
	<children>
		<AnchorPane layoutX="14.0" layoutY="14.0" prefHeight="150.0" prefWidth="450.0">
			<children>
				<Label layoutX="11.0" prefHeight="42.0" prefWidth="500.0" text="Hoppla, da ist ein Fehler passiert!">
					<font>
						<Font name="System Bold" size="15.0" />
					</font>
				</Label>
				<Label layoutX="11.0" layoutY="58.0" prefHeight="42.0" prefWidth="500.0" text="Der Spielername oder das Passwort wurde nicht erkannt. " />
				<Label layoutX="11.0" layoutY="92.0" prefWidth="450.0" text="Bitte versuche dich erneut anzumelden oder erstelle einen neuen Spieler." />
				<ImageView fitHeight="86.0" fitWidth="117.0" layoutX="406.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../Images/logo.png" />
					</image>
				</ImageView>
			</children>
		</AnchorPane>
		<ButtonBar layoutX="336.0" layoutY="216.0" prefHeight="40.0" prefWidth="200.0">
			<buttons>
				<Button fx:id="closeButton" cancelButton="true" mnemonicParsing="false" onAction="#closeButton" text="Schliessen" />
			</buttons>
		</ButtonBar>
		<Hyperlink fx:id="newUser" layoutX="20.0" layoutY="191.0" onAction="#newUser" text="neuen Spieler erstellen" />
		<Hyperlink fx:id="Passwort" layoutX="20.0" layoutY="168.0" onAction="#Passwort" prefHeight="23.0" prefWidth="121.0" text="Passwort vergessen?" />
	</children>
</AnchorPane>
